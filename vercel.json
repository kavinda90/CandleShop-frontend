{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
        "REACT_APP_PUBLIC_URL": "https://candle-shop-api.vercel.app",
        "REACT_APP_API": "https://candle-shop-frontend.vercel.app"
      }
  }